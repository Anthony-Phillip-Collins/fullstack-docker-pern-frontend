{
  "name": "fullstack-docker-pern-frontend",
  "version": "1.1.2",
  "type": "module",
  "description": "Submodule for the frontend of the fullstack-docker-pern project",
  "keywords": [
    "fullstack",
    "docker",
    "postgres",
    "express",
    "react",
    "nginx",
    "typescript",
    "vite"
  ],
  "scripts": {
    "dev": "vite",
    "host": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "preview": "vite preview",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build",
    "docker:dev:build:nc": "npm run docker:dev:build -- --no-cache",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "html-to-text": "^9.0.5",
    "react": "^18.2.0",
    "react-contenteditable": "^3.3.7",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.1",
    "styled-components": "^6.0.5"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "vite": "^4.4.7"
  }
}
